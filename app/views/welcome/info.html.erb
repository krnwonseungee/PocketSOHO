<script src="./js/libs/excanvas.compiled.js"></script>
<![endif]-->


<!-- Plugin JS -->
<%= javascript_include_tag "jquery.carouFredSel-6.2.1-packed.js" %>

  <!-- App JS -->
<%= javascript_include_tag "mvpready-core" %>
<%= javascript_include_tag "mvpready-landing" %>


</body>
</html>
